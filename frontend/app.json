{
  "expo": {
    "name": "TapRight",
    "slug": "tapright",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tapright-logo.png",
    "scheme": "tapright",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "8bd3ad6a-60a3-4cb5-af6c-feba23b93e4d"
      },
      "router": {}
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "TapRight needs your location to recommend the best credit card based on where you are.",
        "NSLocationWhenInUseUsageDescription": "TapRight needs your location to recommend the best credit card based on where you are.",
        "UIBackgroundModes": [
          "location"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.hkamma2004.tapright"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tapright-logo.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tapright-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tapright-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "TapRight needs your location to recommend the best credit card based on where you are.",
          "locationWhenInUsePermission": "TapRight needs your location to recommend the best credit card based on where you are."
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}